{
  "name": "pnlinh/laravel-google-distance",
  "type": "library",
  "description": "Caculate Google Distance Matrix API in Laravel",
  "license": "MIT",
  "authors": [
    {
      "name": "Pham Ngoc Linh",
      "email": "pnlinh1207@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.1.3",
    "guzzlehttp/guzzle": "^7.0"
  },
  "require-dev": {
    "mockery/mockery": "^1.0",
    "phpunit/phpunit": "^8.0",
    "orchestra/testbench": "^4.0"
  },
  "autoload": {
    "psr-4": {
      "Pnlinh\\GoogleDistance\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Pnlinh\\GoogleDistance\\Tests\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Pnlinh\\GoogleDistance\\Providers\\GoogleDistanceServiceProvider"
      ]
    }
  },
  "scripts": {
    "test": [
      "vendor/bin/phpunit"
    ]
  }
}
